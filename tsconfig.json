{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/makeProjectsPage",
    "src/app/components/Blognavbar.jsx",
    "src/app/components/NavbarHome.jsx",
    "src/app/components/page.client.jsx"
, "src/app/blog/layout.tsx", "src/app/components/currentNavbar.jsx", "src/app/components/layout.client.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}